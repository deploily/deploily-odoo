<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="0">
    <record id="payment_provider_cibepay" model="payment.provider">
        <field name="code">cibepay</field>
        <field name="name">CIB/EDAHABIYA Payment Gateway</field>
        <field name="image_128" type="base64" file="deploily_epayment/static/src/images/logo_cib.png" />
        <field name="allow_tokenization">True</field>
        <field name="payment_method_ids"
            eval="[Command.set([
                  ref('deploily_epayment.payment_method_cibepay'),
                  ref('payment.payment_method_card'),

              ])]"
        />
    </record>
</odoo>